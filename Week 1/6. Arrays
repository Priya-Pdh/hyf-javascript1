// using PUSH() Function

const favoriteColors = []
console.log("my favorite colors")
console.log("blue", "pink", "white")


const favoriteAnimals = ["rabbit", "horse", "deer", "dolphin"]
console.log(favoriteAnimals)

favoriteAnimals.push("baby pig") 
console.log(favoriteAnimals)
