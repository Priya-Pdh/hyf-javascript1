//2.create a function colorCar that receives color as a parameter

function colorCar(color){
  return ("a "+ color + " car")
  }
//now call the colorCar and insert new color
colorCar("red");


// Another method:

const colorCar = (color) => {
 console.log("a" + color +"car");
 console.log(`a" + ${color} +"car`); 
}
colorCar("red");

